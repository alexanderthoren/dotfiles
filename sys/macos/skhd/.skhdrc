cmd - return : /Applications/Alacritty.app/Contents/MacOs/alacritty
cmd + alt - x : /Applications/Xcode.app/Contents/MacOs/Xcode

# display
# focus
shift + ctrl - r : yabai -m display --focus recent
shift + ctrl - p : yabai -m display --focus prev
shift + ctrl - n : yabai -m display --focus next

shift + ctrl - 1 : yabai -m display --focus 1
shift + ctrl - 2 : yabai -m display --focus 2
shift + ctrl - 3 : yabai -m display --focus 3

# space
# create - destroy
fn + shift - c : yabai -m space --create
fn + shift - d : yabai -m space --destroy

# layout
fn + shift - b : yabai -m space --layout bsp
fn + shift - f : yabai -m space --layout float
fn + shift - s : yabai -m space --layout stack

fn + shift - 0 : yabai -m space --balance
fn + shift - x : yabai -m space --mirror x-axis
fn + shift - y : yabai -m space --mirror y-axis

# focus
fn + shift - r : yabai -m space --focus recent
fn + shift - p : yabai -m space --focus prev || yabai -m space --focus last
fn + shift - n : yabai -m space --focus next || yabai -m space --focus first

fn + shift - 1 : yabai -m space --focus 1
fn + shift - 2 : yabai -m space --focus 2
fn + shift - 3 : yabai -m space --focus 3
fn + shift - 4 : yabai -m space --focus 4
fn + shift - 5 : yabai -m space --focus 5
fn + shift - 6 : yabai -m space --focus 6
fn + shift - 7 : yabai -m space --focus 7
fn + shift - 8 : yabai -m space --focus 8
fn + shift - 9 : yabai -m space --focus 9

# move
fn + shift + ctrl - p : yabai -m space --move prev
fn + shift + ctrl - n : yabai -m space --move next
fn + shift + ctrl - 1 : yabai -m space --display 1
fn + shift + ctrl - 2 : yabai -m space --display 2
fn + shift + ctrl - 3 : yabai -m space --display 3

# window
# focus
alt - h : yabai -m window --focus west
alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north
alt - l : yabai -m window --focus east
alt - r : yabai -m window --focus recent
alt - p : yabai -m window --focus prev
alt - n : yabai -m window --focus next
alt - m : yabai -m window --focus mouse

# swap
alt + ctrl - h : yabai -m window --swap west
alt + ctrl - j : yabai -m window --swap south
alt + ctrl - k : yabai -m window --swap north
alt + ctrl - l : yabai -m window --swap east
alt + ctrl - r : yabai -m window --swap recent
alt + ctrl - p : yabai -m window --swap prev
alt + ctrl - n : yabai -m window --swap next
alt + ctrl - m : yabai -m window --swap mouse

# warp
alt + cmd - h : yabai -m window --warp west
alt + cmd - j : yabai -m window --warp south
alt + cmd - k : yabai -m window --warp north
alt + cmd - l : yabai -m window --warp east
alt + cmd - r : yabai -m window --warp recent
alt + cmd - p : yabai -m window --warp prev
alt + cmd - n : yabai -m window --warp next
alt + cmd - m : yabai -m window --warp mouse

# move to space 
alt + ctrl + cmd - h : yabai -m window --space prev
alt + ctrl + cmd - l : yabai -m window --space next

# move to display 
alt + ctrl + cmd - p : yabai -m window --display prev
alt + ctrl + cmd - n : yabai -m window --display next
