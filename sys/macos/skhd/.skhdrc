lctrl + lalt + lcmd - return : /Applications/Alacritty.app/Contents/MacOS/Alacritty
lctrl + lalt + lcmd - x : /Applications/Xcode.app/Contents/MacOS/Xcode
lctrl + lalt + lcmd - s : /Applications/Safari.app/Contents/MacOS/Safari

# display
# focus
rcmd - r : yabai -m display --focus recent
rcmd - p : yabai -m display --focus prev
rcmd - n : yabai -m display --focus next

rcmd - 1 : yabai -m display --focus 1
rcmd - 2 : yabai -m display --focus 2
rcmd - 3 : yabai -m display --focus 3

# space
# create - destroy
ralt - c : yabai -m space --create
ralt - d : yabai -m space --destroy

# layout
ralt - b : yabai -m space --layout bsp
ralt - f : yabai -m space --layout float
ralt - s : yabai -m space --layout stack

ralt - 0 : yabai -m space --balance
ralt - x : yabai -m space --mirror x-axis
ralt - y : yabai -m space --mirror y-axis

# focus
ralt - r : yabai -m space --focus recent
ralt - p : yabai -m space --focus prev || yabai -m space --focus last
ralt - n : yabai -m space --focus next || yabai -m space --focus first

ralt - 1 : yabai -m space --focus 1
ralt - 2 : yabai -m space --focus 2
ralt - 3 : yabai -m space --focus 3
ralt - 4 : yabai -m space --focus 4
ralt - 5 : yabai -m space --focus 5
ralt - 6 : yabai -m space --focus 6
ralt - 7 : yabai -m space --focus 7
ralt - 8 : yabai -m space --focus 8
ralt - 9 : yabai -m space --focus 9

# move
ralt + ctrl - p : yabai -m space --move prev
ralt + ctrl - n : yabai -m space --move next
ralt + ctrl - 1 : yabai -m space --display 1
ralt + ctrl - 2 : yabai -m space --display 2
ralt + ctrl - 3 : yabai -m space --display 3

# window
# focus
lalt - h : yabai -m window --focus west
lalt - j : yabai -m window --focus south
lalt - k : yabai -m window --focus north
lalt - l : yabai -m window --focus east
lalt - r : yabai -m window --focus recent
lalt - p : yabai -m window --focus prev
lalt - n : yabai -m window --focus next
lalt - m : yabai -m window --focus mouse

# swap
lalt + ctrl - h : yabai -m window --swap west
lalt + ctrl - j : yabai -m window --swap south
lalt + ctrl - k : yabai -m window --swap north
lalt + ctrl - l : yabai -m window --swap east
lalt + ctrl - r : yabai -m window --swap recent
lalt + ctrl - p : yabai -m window --swap prev
lalt + ctrl - n : yabai -m window --swap next
lalt + ctrl - m : yabai -m window --swap mouse

# warp
lalt + lcmd - h : yabai -m window --warp west
lalt + lcmd - j : yabai -m window --warp south
lalt + lcmd - k : yabai -m window --warp north
lalt + lcmd - l : yabai -m window --warp east
lalt + lcmd - r : yabai -m window --warp recent
lalt + lcmd - p : yabai -m window --warp prev
lalt + lcmd - n : yabai -m window --warp next
lalt + lcmd - m : yabai -m window --warp mouse

# move to space 
lalt + ralt - p : yabai -m window --space prev
lalt + ralt - n : yabai -m window --space next

# move to display 
lalt + rcmd - p : yabai -m window --display prev
lalt + rcmd - n : yabai -m window --display next
